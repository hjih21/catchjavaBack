package com.smhrd.web.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class MyViewController {

	@GetMapping("/")
	public String gohome() {
		
		return "main";
	}
	
	@GetMapping("/login")
	public String login() {
		return "login";  
	}

	
}
